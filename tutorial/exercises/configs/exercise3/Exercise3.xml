<?xml version="1.0" encoding="UTF-8"?>
<society 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  name="Exercise-3" 
  xsi:noNamespaceSchemaLocation="http://www.cougaar.org/2003/society.xsd">
  <host name="localhost">
    <node name="Exercise3">
      <agent name="MyAgent">
        <component class="org.cougaar.tutorial.exercise3.ManagerPlugin"/>
        <component class="org.cougaar.tutorial.exercise3.ManagerAllocatorPlugin"/>
        <component class="org.cougaar.tutorial.exercise3.ProgrammerLDMPlugin"/>
        <component class="org.cougaar.core.servlet.SimpleServletComponent">
          <argument>org.cougaar.planning.servlet.PlanViewServlet</argument>
          <argument>/tasks</argument>
        </component>
        <component class="org.cougaar.core.servlet.SimpleServletComponent">
          <argument>org.cougaar.planning.servlet.CompletionServlet</argument>
          <argument>/completion</argument>
        </component>
      </agent>
    </node>
  </host>
</society>
