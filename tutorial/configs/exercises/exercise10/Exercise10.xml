<?xml version="1.0" encoding="UTF-8"?>
<society 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  name="Exercise-10" 
  xsi:noNamespaceSchemaLocation="http://www.cougaar.org/2003/society.xsd">
  <host name="localhost">
    <node name="Exercise10">
      <agent name="Management">
        <component class="org.cougaar.tutorial.exercise10.ManagerPlugin"/>
        <component class="org.cougaar.tutorial.exercise10.ManagerAllocatorPlugin"/>
        <component class="org.cougaar.planning.plugin.asset.AssetReportPlugin"/>
        <component class="org.cougaar.core.servlet.SimpleServletComponent">
          <argument>org.cougaar.planning.servlet.PlanViewServlet</argument>
          <argument>/tasks</argument>
        </component>
        <component class="org.cougaar.core.servlet.SimpleServletComponent">
          <argument>org.cougaar.planning.servlet.CompletionServlet</argument>
          <argument>/completion</argument>
        </component>
        <component class="org.cougaar.planning.plugin.asset.AssetDataParamPlugin">
          <argument>Prototype:Entity</argument>
          <argument>ClusterPG:MessageAddress:MessageAddress:Management</argument>
          <argument>ItemIdentificationPG:ItemIdentification:String:Management</argument>
          <argument>ItemIdentificationPG:Nomenclature:String:Management</argument>
          <argument>ItemIdentificationPG:AlternateItemIdentification:String:Alice's Department</argument>
          <argument>TypeIdentificationPG:TypeIdentification:String:UTC/RTOrg</argument>
          <argument>TypeIdentificationPG:Nomenclature:String:Project Management</argument>
          <argument>TypeIdentificationPG:AlternateTypeIdentification:String:Alice's Department</argument>
          <argument>EntityPG:Roles:Collection&lt;Role&gt;:ProjectManagement</argument>
        </component>
      </agent>
      <agent name="Development">
        <component class="org.cougaar.tutorial.exercise10.ProgrammerLDMPlugin"/>
        <component class="org.cougaar.tutorial.exercise10.DevelopmentAllocatorPlugin"/>
        <component class="org.cougaar.tutorial.exercise10.DevelopmentExpanderPlugin"/>

        <component class="org.cougaar.tutorial.exercise10.DevelopmentAssessorPlugin"/>
        <component class="org.cougaar.tutorial.exercise10.TakeVacationServlet"/>

        <component class="org.cougaar.planning.plugin.asset.AssetReportPlugin"/>
        <component class="org.cougaar.core.servlet.SimpleServletComponent">
          <argument>org.cougaar.planning.servlet.PlanViewServlet</argument>
          <argument>/tasks</argument>
        </component>
        <component class="org.cougaar.core.servlet.SimpleServletComponent">
          <argument>org.cougaar.planning.servlet.CompletionServlet</argument>
          <argument>/completion</argument>
        </component>
        <component class="org.cougaar.core.servlet.SimpleServletComponent">
          <argument>org.cougaar.tutorial.exercise10.ScheduleServlet</argument>
          <argument>/showSchedule</argument>
        </component>
        <component class="org.cougaar.planning.plugin.asset.AssetDataParamPlugin">
          <argument>Prototype:Entity</argument>
          <argument>ClusterPG:MessageAddress:MessageAddress:Development</argument>
          <argument>ItemIdentificationPG:ItemIdentification:String:Development</argument>
          <argument>ItemIdentificationPG:Nomenclature:String:Development</argument>
          <argument>ItemIdentificationPG:AlternateItemIdentification:String:Bob's Department</argument>
          <argument>TypeIdentificationPG:TypeIdentification:String:UTC/RTOrg</argument>
          <argument>TypeIdentificationPG:Nomenclature:String:Software Development</argument>
          <argument>TypeIdentificationPG:AlternateTypeIdentification:String:Bob's Department</argument>
          <argument>EntityPG:Roles:Collection&lt;Role&gt;:SoftwareDevelopment</argument>
          <argument>Relationship:MessageAddress=Management,ItemIdentification=Management,TypeIdentification=UTC/RTOrg,Role=SoftwareDevelopment,StartTime=01/01/2000 12:00 am,EndTime= </argument>
        </component>
      </agent>
    </node>
  </host>
</society>
