<?xml version='1.0'?>
<society name='Exercise-11A'
  xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
  xsi:noNamespaceSchemaLocation='http://www.cougaar.org/2003/society.xsd'>
  <host name="localhost">
    <node name="Exercise11A">
      <agent name="Management" class="org.cougaar.core.agent.SimpleAgent">
        <component
          name="org.cougaar.tutorial.exercise11.ManagerPlugin"
          class="org.cougaar.tutorial.exercise11.ManagerPlugin"
          priority="COMPONENT"
          insertionpoint="Node.AgentManager.Agent.PluginManager.Plugin">
        </component>
        <component
          name="org.cougaar.tutorial.exercise11.ManagerAllocatorPlugin"
          class="org.cougaar.tutorial.exercise11.ManagerAllocatorPlugin"
          priority="COMPONENT"
          insertionpoint="Node.AgentManager.Agent.PluginManager.Plugin">
        </component>
        <component
          name="org.cougaar.planning.plugin.asset.AssetReportPlugin"
          class="org.cougaar.planning.plugin.asset.AssetReportPlugin"
          priority="COMPONENT"
          insertionpoint="Node.AgentManager.Agent.PluginManager.Plugin">
        </component>
        <component
          name="org.cougaar.core.servlet.SimpleServletComponent(org.cougaar.planning.servlet.PlanViewServlet, /tasks)"
          class="org.cougaar.core.servlet.SimpleServletComponent"
          priority="COMPONENT"
          insertionpoint="Node.AgentManager.Agent.PluginManager.Plugin">
          <argument>org.cougaar.planning.servlet.PlanViewServlet</argument>
          <argument>/tasks</argument>
        </component>
        <component
          name="org.cougaar.core.servlet.SimpleServletComponent(org.couggar.planning.servlet.CompletionServlet, /completion)"
          class="org.cougaar.core.servlet.SimpleServletComponent"
          priority="COMPONENT"
          insertionpoint="Node.AgentManager.Agent.PluginManager.Plugin">
          <argument>org.cougaar.planning.servlet.CompletionServlet</argument>
          <argument>/completion</argument>
        </component>
        <component
          name='org.cougaar.planning.plugin.asset.AssetDataParamPlugin()'
          class='org.cougaar.planning.plugin.asset.AssetDataParamPlugin'
          priority='COMPONENT'
          insertionpoint='Node.AgentManager.Agent.PluginManager.Plugin'>
          <argument>
                  Prototype:Entity
          </argument>
          <argument>
            ClusterPG:MessageAddress:MessageAddress:Management
          </argument>
          <argument>
                  ItemIdentificationPG:ItemIdentification:String:Management
          </argument>
          <argument>
                  ItemIdentificationPG:Nomenclature:String:Management
          </argument>
          <argument>
                  ItemIdentificationPG:AlternateItemIdentification:String:Alice's Department
          </argument>
          <argument>
                  TypeIdentificationPG:TypeIdentification:String:UTC/RTOrg
          </argument>
          <argument>
                  TypeIdentificationPG:Nomenclature:String:Project Management
          </argument>
          <argument>
                  TypeIdentificationPG:AlternateTypeIdentification:String:Alice's Department
          </argument>
          <argument>
                  EntityPG:Roles:Collection&lt;Role&gt;:ProjectManagement
          </argument>
        </component>
      </agent>
     </node>
  </host>
</society>
