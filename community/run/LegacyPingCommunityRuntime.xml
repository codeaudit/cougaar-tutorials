<?xml version='1.0'?>

<!--
Two-node "ping" runtime System Properties.
This runtime will use the legacy template, which implies the legacy community implementation (from lib/community.jar) by default if not specified explicitly.
Thus, the configuration for the community will be taken from communities.xml
-->
<runtime>

  <!-- Set infrastructure template to legacy (default) to get legacy community component loaded (default for legacy), see doc/OnlineManual/Templates.html -->
  <vm_parameter
    name="-Dorg.cougaar.society.xsl.param.template"
    value="legacy"/>

  <!-- Optional log4j config file -->
  <vm_parameter
     name="-Dorg.cougaar.core.logging.config.filename"
     value="logging.props"/>
</runtime>
