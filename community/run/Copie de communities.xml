<?xml version="1.0" encoding="UTF-8"?>
<Communities>
	<Community Name="CommunityA">
		<!-- no CommunityManager is specified explicitely, one is selected among the possible ones -->
		<Entity Name="Agent-1">
			<Attribute ID='EntityType' Value='Agent' />
			<Attribute ID="Role" Value="Member" />
			<Attribute ID="canBeManager" Value="True" />
			<!-- extra application attribute -->
			<Attribute ID="Good-receiver" Value="false" />
		</Entity>
		<!-- EntityType="Agent", Role="Member" and canBeManager="True" are the default values -->
		<Entity Name="Agent-2"/>
		<Entity Name="Agent-3">
			<Attribute ID='EntityType' Value='Agent' />
			<Attribute ID="Role" Value="Member" />
			<!-- can't be CommunityManager -->
			<Attribute ID="canBeManger" Value="False" />
			<!-- extra application attribute -->
			<Attribute ID="Good-receiver" Value="false" />
		</Entity>
		<Entity Name="Agent-4">
			<Attribute ID='EntityType' Value='Agent' />
			<Attribute ID="Role" Value="Member" />
			<!-- can't be CommunityManager -->
			<Attribute ID="canBeManger" Value="No" />
			<!-- extra application attribute -->
			<Attribute ID="Good-receiver" Value="true" />
		</Entity>
		<Entity Name="Agent-5">
			<Attribute ID='EntityType' Value='Agent' />
			<Attribute ID="Role" Value="Member" />
			<!-- extra application attributes -->
			<Attribute ID="Good-receiver" Value="true" />
			<Attribute ID="Champion" Value="true" />
		</Entity>
		<!-- example of nested community -->
		<Entity Name="CommunityD">
			<Attribute ID="EntityType" Value="Community" />
			<Attribute ID="Role" Value="Member" />
		</Entity>		
	</Community>
	<Community Name="CommunityB">
		<!-- it's possible to specify which one is the CommunityManager -->
		<Attribute ID="CommunityManager" Value="Agent-2"/>
		<Entity Name="Agent-2">
			<!-- nothing is specified, it can become the CommunityManager -->
			<Attribute ID='EntityType' Value='Agent' />
			<Attribute ID="Role" Value="Member" />
			<Attribute ID="Good-receiver" Value="false" />
		</Entity>
		<Entity Name="Agent-3">
			<Attribute ID='EntityType' Value='Agent' />
			<Attribute ID="Role" Value="Member" />
			<Attribute ID="Good-receiver" Value="false" />
		</Entity>
	</Community>
	<Community Name="CommunityC">
		<Entity Name="Agent-1">
			<Attribute ID='EntityType' Value='Agent' />
			<Attribute ID="Role" Value="Member" />
		</Entity>
		<Entity Name="Agent-4">
			<Attribute ID='EntityType' Value='Agent' />
			<Attribute ID="Role" Value="Member" />
			<Attribute ID="Good-receiver" Value="true" />
		</Entity>
	</Community>	
	<Community Name="CommunityD">
		<Entity Name="Agent-1">
			<Attribute ID='EntityType' Value='Agent' />
			<Attribute ID="Role" Value="Member" />
		</Entity>
		<Entity Name="Agent-2">
			<Attribute ID='EntityType' Value='Agent' />
			<Attribute ID="Role" Value="Member" />
			<Attribute ID="Good-receiver" Value="true" />
		</Entity>
	</Community>		
</Communities>
